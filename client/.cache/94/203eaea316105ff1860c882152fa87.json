lumn":6},"generated":{"line":168,"column":4}},{"source":"src/styles/index.scss","name":null,"original":{"line":179,"column":6},"generated":{"line":168,"column":25}},{"source":"src/styles/index.scss","name":null,"original":{"line":180,"column":6},"generated":{"line":169,"column":4}},{"source":"src/styles/index.scss","name":null,"original":{"line":180,"column":6},"generated":{"line":169,"column":19}}],"sources":{"src/styles/index.scss":"body {\n  padding: 0;\n  margin: 0;\n  overflow-x: hidden;\n}\n.page {\n  height: 100%;\n  width: 100vw;\n  margin: -3.3em 0 0 0;\n  padding: 0;\n}\n.container {\n  width: 100vw;\n  height: 100vh;\n  display: flex;\n}\n\n/*--------------------------------------------------------------------------------------------------------------------*/\n/*--------------------------------------------------------------------------------------------------------------------*/\n/* Main Styling Landing Page*/\n\n$gray_background: rgb(60, 67, 75);\n$gray_letters: rgb(123, 127, 131);\n$teal_button: rgb(32, 145, 163);\n$book_brown: rgba(77, 63, 63, 0.63);\n\n.body {\n  font-size: 14;\n  font-weight: normal;\n  font-family: Arial, Helvetica, sans-serif;\n  max-height: 100vh;\n  width: 100vw;\n  margin: 0;\n  padding: 0;\n  position: relative;\n  overflow-y: scroll;\n  overflow-x: hidden;\n}\n\n.main {\n  width: 100vw;\n  display: flex;\n  flex-direction: column;\n  text-align: center;\n  align-items: center;\n  height: 100vw;\n  margin: 3.5em 0 0 auto;\n  a {\n    color: white;\n    text-decoration: none;\n  }\n  &__div {\n    margin: 4em auto 0 auto;\n    width: 100%;\n    height: 100%;\n  }\n  &__titlecontainer {\n    height: 44vh;\n    min-height: 280px;\n    width: 101%;\n    text-align: center;\n    justify-content: center;\n    position: relative;\n    overflow: hidden;\n    background-image: url('../images/library2.jpeg');\n    background-repeat: no-repeat;\n    background-position-x: 50%;\n    background-position-y: 45%;\n    background-size: 100%;\n    max-width: 1050px;\n    margin-left: auto;\n    margin-right: auto;\n    top: -5px;\n    padding: 0;\n  }\n  &__titleheadings {\n    margin-top: 120px;\n    width: 100%;\n    background-color: $book_brown;\n    margin-left: auto;\n    margin-right: auto;\n    color: rgb(255, 255, 255);\n  }\n  &__titleheading1 {\n    font-size: 45pt;\n    font-weight: bold;\n  }\n  &__titleheading2 {\n    font-weight: lighter;\n    margin-top: -35px;\n  }\n  &__descriptions {\n    width: 90%;\n    margin: 0.5em auto 0 auto;\n    font-family: 'Times New Roman', Times, serif;\n    font-size: 11pt;\n  }\n  .description__heading {\n    font-style: italic;\n    font-weight: lighter;\n  }\n  .main_paragraph {\n    text-align: left;\n    width: 80%;\n    margin-left: auto;\n    margin-right: auto;\n  }\n  .description__button {\n    height: 30px;\n    width: 180px;\n    font-weight: bold;\n    font-size: 12pt;\n    border-radius: 5px;\n  }\n  .button--dark {\n    background-color: rgb(52, 58, 64);\n    color: white;\n  }\n}\n\n//Breakpoint for Md-laptop 769px - 1024px\n@media screen and (min-width: 500px) and (max-width: 1025px) {\n  .main {\n    &__titlecontainer {\n      height: 42vh;\n      width: 100%;\n      overflow: hidden;\n    }\n    &__titleheadings {\n      margin-top: 15%;\n    }\n  }\n}\n\n//Breakpoint for Lg-desktop 1025px - 1200px\n@media screen and (min-width: 1025px) and (max-width: 2200px) {\n  .body {\n    justify-content: center;\n  }\n\n  .main {\n    width: 100vw;\n    margin-left: -7px;\n\n    &__titlecontainer {\n      background-position-y: 40%;\n    }\n    &__titleheadings {\n      margin-top: 15%;\n    }\n  }\n}\n\n//Breakpoint for Mobile 375px - 480px\n@media screen and (min-width: 0px) and (max-width: 480px) {\n  .page {\n    height: fit-content;\n    width: 100%;\n    padding: 0;\n    margin-left: -10px;\n  }\n  .main {\n    width: 100vw;\n    display: flex;\n    &__titlecontainer {\n      min-width: 102vw;\n      justify-content: center;\n      height: 33%;\n      margin-top: -8%;\n      margin-left: 5px;\n      background-size: 150%;\n    }\n\n    &__titleheadings {\n      margin-top: 15%;\n      width: 100%;\n    }\n    &__descriptions {\n      padding-bottom: 100px;\n      margin-left: 8%;\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"345b64025b97b11affa599258e45986e","cacheData":{"env":{}}}